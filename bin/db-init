#!/usr/bin/env python

from sqlalchemy import create_engine
from jae.db.models import BaseModel as model
from jae.common import cfg

CONF=cfg.CONF

 
engine_url = CONF.db_connection
try:
    engine = create_engine(engine_url)
except:
    raise

try:
    model.metadata.create_all(engine)
except:
    raise
